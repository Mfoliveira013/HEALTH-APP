{
  "version": 2,
  "builds": [
    {
      "src": "server-supabase.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|html))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server-supabase.js"
    }
  ]
}